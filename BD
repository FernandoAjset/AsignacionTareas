  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;
--------------------------------------------------------
--  DDL for Table USUARIOS
--------------------------------------------------------

  CREATE TABLE "ADMIN_DBA"."USUARIOS" 
   (	"ID_USUARIO" NUMBER(*,0), 
	"PUESTO" VARCHAR2(30 BYTE), 
	"NOMBRE" VARCHAR2(30 BYTE), 
	"CONTRASEÑA" VARCHAR2(30 BYTE), 
	"EMAIL" VARCHAR2(30 BYTE), 
	"ID_TIPO" NUMBER(*,0), 
	"ID_DEPARTAMENTO" NUMBER(*,0), 
	"USUARIO" VARCHAR2(30 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;
--------------------------------------------------------
--  DDL for View ATRASADAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "ADMIN_DBA"."ATRASADAS" ("ID_USUARIO", "TAREAS") AS 
  SELECT ID_USUARIO, COUNT(*) TAREAS
FROM TAREA
WHERE SYSDATE -FECHAASIGNACION >=15 AND ESTADO=0
GROUP BY ID_USUARIO
ORDER BY COUNT(*) DESC
;
--------------------------------------------------------
--  DDL for View MAXIMO
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "ADMIN_DBA"."MAXIMO" ("ID_USUARIO", "TAREAS") AS 
  SELECT ID_USUARIO, COUNT(*) TAREAS
FROM TAREA
WHERE ESTADO=0
GROUP BY ID_USUARIO
ORDER BY COUNT(*) DESC
;
--------------------------------------------------------
--  DDL for View TAREAULTIMOSMESES
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "ADMIN_DBA"."TAREAULTIMOSMESES" ("ID_USUARIO", "USUARIO", "ID_DEPARTAMENTO", "HACE_1_MES", "HACE_2_MESES", "HACE_3_MESES") AS 
  SELECT t.id_usuario,u.usuario,d.id_departamento,
nvl(TAREASMES(t.id_usuario,1),0) HACE_1_MES,
nvl(TAREASMES(t.id_usuario,2),0) HACE_2_MESES,
nvl(TAREASMES(t.id_usuario,3),0) HACE_3_MESES
FROM tarea t
LEFT JOIN usuarios u
ON t.id_usuario=u.id_usuario
LEFT JOIN departamento d
ON u.id_departamento=d.id_departamento
GROUP BY t.id_usuario,u.usuario,d.id_departamento
order by t.id_usuario ASC
;
--------------------------------------------------------
--  DDL for View ULTIMOSCINCODIAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "ADMIN_DBA"."ULTIMOSCINCODIAS" ("ID_USUARIO", "USUARIO", "ID_DEPARTAMENTO", "HACE_1_DIAS", "HACE_2_DIAS", "HACE_3_DIAS", "HACE_4_DIAS", "HACE_5_DIAS") AS 
  SELECT t.id_usuario,u.usuario,d.id_departamento,
nvl(tareafecha(t.id_usuario,1),0) HACE_1_DIAS, 
nvl(tareafecha(t.id_usuario,2),0) HACE_2_DIAS, 
nvl(tareafecha(t.id_usuario,3),0) HACE_3_DIAS,
nvl(tareafecha(t.id_usuario,4),0) HACE_4_DIAS,
nvl(tareafecha(t.id_usuario,5),0) HACE_5_DIAS
FROM tarea t
LEFT JOIN usuarios u
ON t.id_usuario=u.id_usuario
LEFT JOIN departamento d
ON u.id_departamento=d.id_departamento
GROUP BY t.id_usuario,u.usuario,d.id_departamento
order by t.id_usuario ASC
;
