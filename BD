--------------------------------------------------------
--  DDL for Function TAREAFECHA
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN_DBA"."TAREAFECHA" (idUsuario NUMBER, dias NUMBER)RETURN NUMBER IS
vTareas number;
BEGIN
SELECT nvl(count(id_tarea),0) tareas into vTareas from tarea where id_usuario=idUsuario 
and trunc(fechaasignacion)=trunc(sysdate-dias);
return vTareas;
END;

/
--------------------------------------------------------
--  DDL for Function TAREASMES
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN_DBA"."TAREASMES" (idUsuario NUMBER, mes NUMBER)RETURN NUMBER IS
vTareas number;
BEGIN
SELECT COUNT(*) tareas INTO vTareas FROM tarea WHERE id_usuario=idUsuario and ESTADO=1 AND EXTRACT(MONTH FROM fechafinalizacion)=EXTRACT(MONTH FROM sysdate)-mes
group by id_usuario;
return vTareas;
END;



/
